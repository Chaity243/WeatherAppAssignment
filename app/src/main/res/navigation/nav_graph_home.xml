    android:id="@+id/nav_graph_home"
    app:startDestination="@id/homeFragment">
    <action
            android:id="@+id/nav_to_home_fragment"
            app:destination="@id/homeFragment"
    />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.msil.sharedmobility.subscribe.presentation.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home">
        <action android:id="@+id/action_homeFragment_to_vehicleSelectFragment"
                app:destination="@id/vehicleSelectFragment" app:launchSingleTop="true"/>
        <argument android:name="city" app:argType="string" app:nullable="true" android:defaultValue=" "/>

    </fragment>
    <action
            android:id="@+id/nav_to_vehicle_fragment"
            app:destination="@id/vehicleSelectFragment"
    />
    <fragment android:id="@+id/vehicleSelectFragment"
              android:name="com.msil.sharedmobility.subscribe.presentation.ui.home.components.VehicleSelectFragment"
              android:label="fragment_vehicle_select" tools:layout="@layout/fragment_vehicle_select"/>

